{
    "name": "tabularasadev/contao-base",
    "description": "Module de Base",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Jimmy",
            "email": "jimmy@tabularasa.fr"
        }
    ],
    "require": {
        "contao/core-bundle": "^4.10 | ^4.12",
        "twig/twig": "^3.3"
    },
    "license": "Not free",
    "autoload": {
        "psr-4": {
            "trdev\\ContaoBaseBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "trdev\\ContaoBaseBundle\\ContaoManager\\Plugin"
    }
}